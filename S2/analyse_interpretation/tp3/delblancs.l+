%option noyywrap
%%
^[\t ]*\n   {/*lignes blanches*/}
^[\t ]+     {/*debuts blancs*/}
[\t ]+$     {/*fins blanches*/}
[\t ]+      {int c;
             if ((c=input())!=EOF && c!=0){
                 unput(c);
                 putchar(' ');
                }
            }
%%
/* ZONE DES FONCTIONS C */
int main(){
    while (yylex()!=0){}
    return 0;
}   
